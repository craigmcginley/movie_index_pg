<h2>Actors</h2>
<ul>
  <% @all_actors.each do |actor| %>
    <li>
      <a href="/actors/<%= actor["id"] %>"><%= actor["name"] %></a>
    </li>
  <% end %>
</ul>

<% if @page_number != 1 %>
  <a href="/actors?page=<%= @page_number - 1 %>">Back</a>
<% end %>
<% if @page_number != @last_page %>
  <a href="/actors?page=<%= @page_number + 1 %>">Next</a>
<% end %>
